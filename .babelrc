{
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false
        }],
        "@babel/preset-react",
        "@babel/preset-flow"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-export-default-from",
        "@babel/plugin-proposal-export-namespace-from",
        "transform-es2015-modules-commonjs",
        "dynamic-import-node"
      ],
    },
    "ssr": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false
        }],
        "@babel/preset-react",
        "@babel/preset-flow"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties"
      ]
    },
    "development": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false
        }],
        "@babel/preset-react",
        "@babel/preset-flow"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-export-default-from",
        "@babel/plugin-proposal-export-namespace-from",
        [
          "@babel/plugin-transform-runtime", {
            "corejs": 3
          }
        ],
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false
        }],
        "@babel/preset-react",
        "@babel/preset-flow"
      ],
      "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-export-default-from",
        "@babel/plugin-proposal-export-namespace-from",
        [
          "@babel/plugin-transform-runtime", {
            "corejs": 3
          }
        ]
      ]
    }
  }
}
